; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Logger",
"language": "GDScript",
"path": "res://Scripts/LogManager.gd"
}, {
"base": "Node",
"class": "PaletteGenerator",
"language": "GDScript",
"path": "res://Scripts/PaletteGenerator.gd"
}, {
"base": "Node",
"class": "PaletteReader",
"language": "GDScript",
"path": "res://Scripts/PaletteReader.gd"
} ]
_global_script_class_icons={
"Logger": "",
"PaletteGenerator": "",
"PaletteReader": ""
}

[application]

config/name="Hyperfight Palette Editor"
run/main_scene="res://Scenes/Menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="HYPERFIGHT"
run/low_processor_mode=true
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )

[autoload]

Global="*res://Scripts/global.gd"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
